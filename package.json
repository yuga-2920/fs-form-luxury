{
  "name": "fs-form",
  "version": "1.0.0",
  "description": "Fashion Styling Form with comprehensive questionnaire",
  "scripts": {
    "deploy": "echo 'Use vercel, netlify, or other static hosting service'",
    "deploy-vercel": "vercel --prod",
    "deploy-netlify": "echo 'Drag and drop the public folder to Netlify'",
    "serve": "npx serve public",
    "preview": "npx serve public",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "build:ts": "tsc"
  },
  "keywords": [
    "fashion",
    "styling",
    "form",
    "questionnaire"
  ],
  "author": "",
  "license": "ISC",
  "main": "analyze_travel.js",
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.53.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "tinybench": "^4.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
